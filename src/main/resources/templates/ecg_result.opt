                <archetype_id>
                    <value>openEHR-EHR-OBSERVATION.ecg_result.v1</value>
                </archetype_id>
                <term_definitions code="at0000">
                    <items id="text">EKG Ergebnis</items>
                    <items id="description">Messung der vom Herzen erzeugten elektrischen Aktivität.</items>
                    <items id="comment">Auch als Elektrokardiogramm oder ECG bezeichnet.</items>
                </term_definitions>
                <term_definitions code="at0001">
                    <items id="text">Event Series</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0002">
                    <items id="text">Beliebiges Ereignis</items>
                    <items id="description">Standardmäßiger, nicht näher beschriebener Zeitpunkt oder Intervall Ereignis, welches in einem Template oder bei der Anwendung genauer definiert werden kann.</items>
                </term_definitions>
                <term_definitions code="at0003">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0005">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="text">Globale QT-Zeit</items>
                    <items id="description">Zeitintervall vom Anfang des QRS Komplexes bis zum Ende der T-Welle über mehrere Ableitungen hinweg.</items>
                </term_definitions>
                <term_definitions code="at0008">
                    <items id="text">Globale QTc-Zeit</items>
                    <items id="description">Frequenzkorrigierte QT-Zeit.</items>
                </term_definitions>
                <term_definitions code="at0009">
                    <items id="text">Geräteinterpretation</items>
                    <items id="description">Ein vom EKG Gerät interpretiertes EKG-Ergebnis.</items>
                    <items id="comment">Es können mehrere Kommentare sein. Die Kodierung der Interpretation mittels einer Terminologie ist, falls möglich, bevorzugt. Zum Beispiel: Normaler Sinusrhythmus; ischämische ST-T-Strecken-Veränderungen in den anterioren Ableitungen; und/oder schlechte R-Progression in der rechten präkordialen Ableitung.</items>
                </term_definitions>
                <term_definitions code="at0012">
                    <items id="text">Globale PQ-Zeit</items>
                    <items id="description">Intervalmessung vom Beginn der P-Welle bis zum Beginn des QRS-Komplex im EKG als Ganzes (Mehrere Ableitungen).</items>
                    <items id="comment">Die PQ-Zeit wird vom Beginn der P-Welle bis zum Beginn des QRS-Komplex gemessen.</items>
                </term_definitions>
                <term_definitions code="at0013">
                    <items id="text">Herzfrequenz</items>
                    <items id="description">Die Frequenz der elektrischen Kontraktionen des Ventrikels im EKG als Ganzes (mehrere Ableitungen).</items>
                    <items id="comment">Auch bekannt als Kammerfrequenz, QRS-Frequenz oder elektrische Herzfrequenz. Die Herzfrequenz wird von R-Welle zu R-Welle gemessen.</items>
                </term_definitions>
                <term_definitions code="at0014">
                    <items id="text">Globale QRS-Dauer</items>
                    <items id="description">Dauer des QRS Komplex, gemessen von dessen Beginn bis zum Anfang des ST Segments (J-Punkt) über mehrere Ableitungen hinweg.</items>
                    <items id="comment">Die QRS-Dauer wird vom Beginn des QRS Komplexes bis zum Beginn des ST Segments (J-Punkt) gemessen.</items>
                </term_definitions>
                <term_definitions code="at0020">
                    <items id="text">P-Achse</items>
                    <items id="description">Durchschnittliche Ausrichtung der elektrischen Aktivität während der atriellen Depolarisation.</items>
                    <items id="comment">Zum Beispiel: Normale Achse oder Linksabweichung; oder Gradzahl.</items>
                </term_definitions>
                <term_definitions code="at0021">
                    <items id="text">QRS-Achse</items>
                    <items id="description">Durchschnittliche Ausrichtung der elektrischen Aktivität während der ventrikulären Depolarisation.</items>
                    <items id="comment">Zum Beispiel: Normale Achse oder Linksabweichung; oder Gradzahl.</items>
                </term_definitions>
                <term_definitions code="at0022">
                    <items id="text">T-Achse</items>
                    <items id="description">Durchschnittliche Ausrichtung der elektrischen Aktivität während der atriellen Repolarisation.</items>
                    <items id="comment">Zum Beispiel: Normale Achse oder Linksabweichung; oder Gradzahl.</items>
                </term_definitions>
                <term_definitions code="at0025">
                    <items id="text">QTc Algorithmus</items>
                    <items id="description">Algorithmus, der verwendet wurde um das QT-Interval zu korrigieren.</items>
                </term_definitions>
                <term_definitions code="at0027">
                    <items id="text">Pro Ableitung</items>
                    <items id="description">Details zu den gemessenen Parametern für jede genannte Ableitung (angegeben in der Einschränkung des Laufzeitnamens).</items>
                </term_definitions>
                <term_definitions code="at0028">
                    <items id="text">P-Dauer</items>
                    <items id="description">Dauer der P-Welle.</items>
                </term_definitions>
                <term_definitions code="at0029">
                    <items id="text">Ableitung I</items>
                    <items id="description">Ableitung I ist die Spannungsdifferenz zwischen der linken Armelektrode und der rechten Armelektrode, die bei Null Grad auf den linken Arm gerichtet ist.</items>
                </term_definitions>
                <term_definitions code="at0030">
                    <items id="text">Ableitung II</items>
                    <items id="description">Ableitung II ist die Spannungsdifferenz zwischen der linken Beinelektrode und der rechten Armelektrode, die bei +60 Grad auf das linke Bein gerichtet ist.</items>
                </term_definitions>
                <term_definitions code="at0031">
                    <items id="text">Ableitung III</items>
                    <items id="description">Ableitung III ist die Spannungsdifferenz zwischen der linken Beinelektrode und der linken Armelektrode, die bei +120 Grad auf das linke Bein gerichtet ist.</items>
                </term_definitions>
                <term_definitions code="at0032">
                    <items id="text">Ableitung aVR</items>
                    <items id="description">Die aVR Ableitung hat die positive Elektrode am rechten Arm und der negative Pol ist eine Kombination aus der linken Armelektrode und der linken Beinelektrode. Sie ist bei -150 Grad auf den rechten Arm gerichtet.</items>
                </term_definitions>
                <term_definitions code="at0033">
                    <items id="text">Ableitung aVL</items>
                    <items id="description">Die aVL Ableitung hat die positive Elektrode am linken Arm und der negative Pol ist eine Kombination aus der rechten Armelektrode und der linken Beinelektrode. Sie ist bei -30 Grad auf die linke Armelektrode ausgerichtet.</items>
                </term_definitions>
                <term_definitions code="at0034">
                    <items id="text">Ableitung aVF</items>
                    <items id="description">Die aVF Ableitung hat die positive Elektrode am linken Bein und der negative Pol ist eine Kombination aus rechter Armelektrode und linker Armelektrode. Sie ist bei +90 Grad richtung linker Beinelektrode ausgerichtet.</items>
                </term_definitions>
                <term_definitions code="at0035">
                    <items id="text">Ableitung V1</items>
                    <items id="description">Eine präkordiale Ableitung, die rechts vom Sternum im vierten Interkostalraum platziert wird. Sie wird auch als Ableitung V2R bezeichnet.</items>
                </term_definitions>
                <term_definitions code="at0036">
                    <items id="text">Ableitung V2</items>
                    <items id="description">Eine präkordiale Ableitung, die links vom Sternum im vierten Interkostalraum platziert wird. Sie wird auch als Ableitung V1R bezeichnet.</items>
                </term_definitions>
                <term_definitions code="at0037">
                    <items id="text">Ableitung V3</items>
                    <items id="description">Eine präkordiale Ableitung, die zwischen Ableitung V2 und V4 platziert wird.</items>
                </term_definitions>
                <term_definitions code="at0038">
                    <items id="text">Ableitung V4</items>
                    <items id="description">Eine präkordiale Ableitung, die im linken fünften Interkostalraum auf der Midklavikularlinie platziert wird.</items>
                </term_definitions>
                <term_definitions code="at0039">
                    <items id="text">Ableitung V5</items>
                    <items id="description">Eine präkordiale Ableitung, die auf gleicher Höhe wie Ableitung V4 auf der linken anterioren Axillarlinie platziert wird.</items>
                </term_definitions>
                <term_definitions code="at0040">
                    <items id="text">Ableitung V6</items>
                    <items id="description">Eine präkordiale Ableitung, die auf gleicher Höhe wie Ableitung V5 auf der linken Midaxillarlinie platziert wird.</items>
                </term_definitions>
                <term_definitions code="at0041">
                    <items id="text">P-Amplitude</items>
                    <items id="description">Amplitude der P-Welle.</items>
                </term_definitions>
                <term_definitions code="at0042">
                    <items id="text">P-Fläche</items>
                    <items id="description">Fläche einer monophasischen P-Welle oder die Fläche des initialen Anteils einer biphasischen P-Welle.</items>
                </term_definitions>
                <term_definitions code="at0043">
                    <items id="text">P'-Amplitude</items>
                    <items id="description">Amplitude der P'-Welle.</items>
                </term_definitions>
                <term_definitions code="at0044">
                    <items id="text">P'-Dauer</items>
                    <items id="description">Dauer der P'-Welle.</items>
                </term_definitions>
                <term_definitions code="at0046">
                    <items id="text">P'-Fläche</items>
                    <items id="description">Fläche des terminalen Anteils einer biphasischen P-Welle.</items>
                </term_definitions>
                <term_definitions code="at0048">
                    <items id="text">Q-Amplitude</items>
                    <items id="description">Amplitude der Q-Welle.</items>
                </term_definitions>
                <term_definitions code="at0049">
                    <items id="text">Q-Dauer</items>
                    <items id="description">Dauer der Q-Welle.</items>
                </term_definitions>
                <term_definitions code="at0050">
                    <items id="text">R-Amplitude</items>
                    <items id="description">Amplitude der R-Welle.</items>
                </term_definitions>
                <term_definitions code="at0051">
                    <items id="text">R-Dauer</items>
                    <items id="description">Dauer der R-Welle.</items>
                </term_definitions>
                <term_definitions code="at0053">
                    <items id="text">S-Amplitude</items>
                    <items id="description">Amplitude der S-Welle.</items>
                </term_definitions>
                <term_definitions code="at0054">
                    <items id="text">S-Dauer</items>
                    <items id="description">Dauer der S-Welle.</items>
                </term_definitions>
                <term_definitions code="at0055">
                    <items id="text">R'-Amplitude</items>
                    <items id="description">Amplitude der R'-Welle.</items>
                </term_definitions>
                <term_definitions code="at0056">
                    <items id="text">R'-Dauer</items>
                    <items id="description">Dauer der R'-Welle.</items>
                </term_definitions>
                <term_definitions code="at0057">
                    <items id="text">S'-Amplitude</items>
                    <items id="description">Amplitude der S'-Welle.</items>
                </term_definitions>
                <term_definitions code="at0058">
                    <items id="text">S'-Dauer</items>
                    <items id="description">Dauer der S'-Welle.</items>
                </term_definitions>
                <term_definitions code="at0059">
                    <items id="text">Ventrikuläre Aktivierungszeit</items>
                    <items id="description">Interval vom Anfang des QRS Komplex bis zum letzten positiven Peak im Komplex oder der letzten wesentlichen Kerbe des letzten Peaks (Je nachdem was später kommt).</items>
                </term_definitions>
                <term_definitions code="at0060">
                    <items id="text">QRS p-p</items>
                    <items id="description">Amplitude von Peak zu Peak der QRS-Dauer.</items>
                </term_definitions>
                <term_definitions code="at0061">
                    <items id="text">QRS-Dauer</items>
                    <items id="description">Dauer des QRS-Komplexes.</items>
                    <items id="comment">Gemessen vom dessen Anfang bis zum Anfang des ST-Segments (J-Punkt).</items>
                </term_definitions>
                <term_definitions code="at0062">
                    <items id="text">QRS-Fläche</items>
                    <items id="description">Fläche des QRS-Komplexes.</items>
                </term_definitions>
                <term_definitions code="at0063">
                    <items id="text">ST-Amplitude am J-Punkt.</items>
                    <items id="description">Erhebung oder Vertiefung am Beginn (J-Punkt) des ST-Segments.</items>
                </term_definitions>
                <term_definitions code="at0064">
                    <items id="text">ST-Mittelpunkt</items>
                    <items id="description">Erhebung oder Vertiefung in der Mitte des ST-Segments.</items>
                </term_definitions>
                <term_definitions code="at0065">
                    <items id="text">ST 80ms</items>
                    <items id="description">Erhebung oder Vertiefung des ST-Segments 80 ms nach Ende des QRS-Komplexes (J-Punkt).</items>
                </term_definitions>
                <term_definitions code="at0066">
                    <items id="text">ST-Ende</items>
                    <items id="description">Erhebung oder Vertiefung am Ende des ST-Segments.</items>
                </term_definitions>
                <term_definitions code="at0067">
                    <items id="text">ST-Dauer</items>
                    <items id="description">Dauer des ST-Segments.</items>
                </term_definitions>
                <term_definitions code="at0068">
                    <items id="text">ST-Steigung</items>
                    <items id="description">Steigung des ST-Segments.</items>
                    <items id="comment">Die Steigung wird in Grad gemessen, bei einer Schreibgeschwindigkeit von 25 mm/sec und einer Skalierung von 1mV/cm.</items>
                </term_definitions>
                <term_definitions code="at0069">
                    <items id="text">ST-Segment Morphologie</items>
                    <items id="description">Form des ST-Segments.</items>
                    <items id="comment">Das Valueset stammt von DICOM PS3.16 2018 c - Content Mapping Resource -http://dicom.nema.org/medical/dicom/current/output/html/part16.html#sect_CID_3233</items>
                </term_definitions>
                <term_definitions code="at0070">
                    <items id="text">ST-Senkung - horizontal</items>
                    <items id="description">Die ST-Strecke weist eine Absenkung auf, jedoch keine fallende Steigung. Dies ist eine spezifischere Form der "ST-Senkung".</items>
                </term_definitions>
                <term_definitions code="at0071">
                    <items id="text">ST-Senkung - aszendierend</items>
                    <items id="description">Die ST-Strecke weist eine aszendierende (aufsteigende) Absenkung auf. Dies ist eine spezifischere Form der "ST-Senkung".</items>
                </term_definitions>
                <term_definitions code="at0072">
                    <items id="text">ST-Senkung - deszendierend</items>
                    <items id="description">Die ST-Strecke weist eine deszendierende (absteigende) Absenkung auf. Dies ist eine spezifischere Form der "ST-Senkung".</items>
                </term_definitions>
                <term_definitions code="at0073">
                    <items id="text">T-Amplitude</items>
                    <items id="description">Amplitude der T-Welle.</items>
                </term_definitions>
                <term_definitions code="at0074">
                    <items id="text">T-Dauer</items>
                    <items id="description">Dauer der T-Welle.</items>
                </term_definitions>
                <term_definitions code="at0075">
                    <items id="text">T-Fläche</items>
                    <items id="description">Fläche der T-Welle.</items>
                </term_definitions>
                <term_definitions code="at0076">
                    <items id="text">Aufnahmegerät</items>
                    <items id="description">Details über das Elektrokardiografiegerät, welches zur Aufnahme des EKG verwendet wurde.</items>
                </term_definitions>
                <term_definitions code="at0077">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0078">
                    <items id="text">Neigung</items>
                    <items id="description">Die kraniokaudale Neigung der Oberfläche auf der die Person während des EKGs liegt.</items>
                </term_definitions>
                <term_definitions code="at0079">
                    <items id="text">Störfaktoren</items>
                    <items id="description">Kommentar und Aufzeichnung zu anderen Faktoren, welche die Messung beeinflussen können.</items>
                    <items id="comment">Zum Beispiel: Unkooperativer Patient; Schwierigkeiten bei der Platzierung der Elektrode; oder feuchte Haut.</items>
                </term_definitions>
                <term_definitions code="at0080">
                    <items id="text">Grad der Anstrengung</items>
                    <items id="description">Details über die körperliche Aktivität während der EKG-Aufzeichnung.</items>
                </term_definitions>
                <term_definitions code="at0081">
                    <items id="text">EKG Diagnose</items>
                    <items id="description">Einzelnes Wort, Satz oder kurze Beschreibung, welche die übergreifende klinische Bedeutung und Signifikanz des EKG Ergebnisses darstellt.</items>
                    <items id="comment">Zum Beispiel: 'Posteriorer Myokardinfarkt' oder 'supraventrikuläre Tachykardie'. Die Kodierung der Diagnose mit einer Terminologie ist, falls möglich, bevorzugt. Die Diagnose sollte mit dem Text des Datenpunkt 'Schlussfolgerung' abgestimmt sein.</items>
                </term_definitions>
                <term_definitions code="at0082">
                    <items id="text">Anzeigegerät</items>
                    <items id="description">Details des Gerätes, welches zur Anzeige der EKG Ausgabe verwendet wurde.</items>
                </term_definitions>
                <term_definitions code="at0083">
                    <items id="text">Multimedia Repräsentation</items>
                    <items id="description">Digitale Darstellung des Ergebnisses.</items>
                    <items id="comment">Das EKG kann als Bilddaten oder anderen multimedia/binären Formaten repräsentiert werden. Mehrere Formate sind zulässig, sollten aber gleichwertige klinische Inhalte darstellen.</items>
                </term_definitions>
                <term_definitions code="at0084">
                    <items id="text">Ableitung V7</items>
                    <items id="description">Ableitung V7 ist eine posteriore Ableitung, die in der posterioren Axillarlinie platziert wird.</items>
                </term_definitions>
                <term_definitions code="at0085">
                    <items id="text">Ableitung V8</items>
                    <items id="description">Ableitung V8 ist eine posteriore Ableitung, die in der Midskapularregion platziert wird.</items>
                </term_definitions>
                <term_definitions code="at0086">
                    <items id="text">Ableitung V9</items>
                    <items id="description">Ableitung V9 ist eine posteriore Ableitung, die in der Paraspinalregion platziert wird.</items>
                </term_definitions>
                <term_definitions code="at0087">
                    <items id="text">Ableitung V4R</items>
                    <items id="description">Eine präkordiale Ableitung, die im rechten fünften Interkostalraum an der Mittelklavikularlinie platziert wird.</items>
                </term_definitions>
                <term_definitions code="at0088">
                    <items id="text">Erweiterung</items>
                    <items id="description">Weitere Informationen, die benötigt werden um lokale Inhalte abzubilden oder zur Anpassung an andere Referenzmodelle.</items>
                    <items id="comment">Zum Beispiel: Lokale Informationsanforderungen oder weitere Metadaten zur Anpassung an FHIR oder CIMI Äquivalente.</items>
                </term_definitions>
                <term_definitions code="at0089">
                    <items id="text">Schlussfolgerung</items>
                    <items id="description">Schlussfolgernder Text über alle wichtigen Befunde im EKG Ergebnis.</items>
                    <items id="comment">Die Schlussfolgerung soll eine erweiterte Version des Datenfeldes 'EKG Diagnose' sein. Zum Beispiel: 'Normaler Sinusrhythmus mit ischämischen ST-T-Strecken Veränderungen in der anterioren Ableitung und schlechte R-Progression in den rechten präkordialen Ableitungen'; oder Vergleich mit früheren EKG Ergebnissen.</items>
                </term_definitions>
                <term_definitions code="at0090">
                    <items id="text">Kommentar</items>
                    <items id="description">Zusätzlicher Text über das EKG Ergebnis, der Sachverhalte beschreibt, die noch nicht durch andere Datenfelder abgedeckt wurden.</items>
                </term_definitions>
                <term_definitions code="at0091">
                    <items id="text">Ableitung V5R</items>
                    <items id="description">Eine präkordiale Ableitung, die auf gleicher Höhe mit der Ableitung V4R an der rechten vorderen Axillarlinie platziert wird.</items>
                </term_definitions>
                <term_definitions code="at0092">
                    <items id="text">Ableitung V6R</items>
                    <items id="description">Eine präkordiale Ableitung, die auf gleicher Höhe mit der Ableitung V5R in der rechten Midaxillarlinie platziert wird.</items>
                </term_definitions>
                <term_definitions code="at0093">
                    <items id="text">Ableitung V3R</items>
                    <items id="description">Eine präkordiale Ableitung, die direkt auf der rechten Seite der Brust zwischen den Ableitungen V1 und V4R angebracht wird.</items>
                </term_definitions>
                <term_definitions code="at0094">
                    <items id="text">Atriale Frequenz</items>
                    <items id="description">Die Frequenz der elektrischen atrialen Kontraktionen im EKG als Ganzes (mehrere Ableitungen).</items>
                    <items id="comment">Auch bekannt als Vorhoffrequenz oder PP-Intervall. Sie wird von P-Welle zu P-Welle gemessen.</items>
                </term_definitions>
                <term_definitions code="at0095">
                    <items id="text">Kommentar zur Geräteinterpretation</items>
                    <items id="description">Kommentar zur Geräteinterpretation</items>
                    <items id="comment">Zum Beispiel: Identifikation des verwendeten Algorithmus; oder Beschreibung der technischen Einschränkungen</items>
                </term_definitions>
                <term_definitions code="at0096">
                    <items id="text">Bereitgestellte klinische Informationen</items>
                    <items id="description">Beschreibung der klinischen Informationen, die zum Zeitpunkt der Interpretation des Ergebnisses zur Verfügung stehen.</items>
                    <items id="comment">Dieses Datenelement kann einen Link zu den ursprünglichen klinischen Informationen, die in der ECG Anfrage zur Verfügung gestellt wurden, beinhalten.</items>
                </term_definitions>
                <term_definitions code="at0097">
                    <items id="text">EKG Elektrodenplatzierung</items>
                    <items id="description">Elektrodenplatzierung zur EKG Aufnahme.</items>
                    <items id="comment">Die Kodierung der Elektrodenplatzierung mit einer Terminologie ist, falls möglich, bevorzugt. Zum Beispiel: ISO/IEEE 11073 MDC codes.</items>
                </term_definitions>
                <term_definitions code="at0098">
                    <items id="text">Beschreibung</items>
                    <items id="description">Beschreibender Text über die Befunde der angegebenen Ableitung.</items>
                    <items id="comment">Zum Beispiel: ST-Segment Befunde.</items>
                </term_definitions>
                <term_definitions code="at0100">
                    <items id="text">EKG-Typ</items>
                    <items id="description">Art des durchgeführten EKGs.</items>
                    <items id="comment">Kodierung des EKG-Typs mit einer Terminologie ist, falls möglich, bevorzugt. Zum Beispiel: 'Standard EKG' oder '15-Kanal EKG'.</items>
                </term_definitions>
                <term_definitions code="at0101">
                    <items id="text">Befund</items>
                    <items id="description">Einzelnes Wort, Satz oder kurze Beschreibung die einen signifikanten Befund des EKG Ergebnisses darstellt.</items>
                    <items id="comment">Zum Beispiel: 'Normales EKG', 'Mehrdeutiges EKG' oder 'Abnormes EKG'; 'Spikes von Schrittmacher Signal'; 'ST-Senkung'; oder 'Vorhofflimmern'. Die Kodierung des Befundes mit einer Terminologie ist, falls möglich, bevorzugt.</items>
                </term_definitions>
                <term_definitions code="at0102">
                    <items id="text">Technische Qualität</items>
                    <items id="description">Einzelnes Wort, Satz oder kurze Beschreibung, die ein signifikantes technisches Qualitätsproblem darstellt, welches das EKG Ergebnis beeinträchtigt.</items>
                    <items id="comment">Zum Beispiel: Kodierung aus der IHE EKG Codetabelle, wie 'Unvollständiges EKG'; 'Gliedmaßenelektroden vertauscht'; oder 'Muskeltremor'</items>
                </term_definitions>
                <term_definitions code="at0103">
                    <items id="text">ST-Hebung</items>
                    <items id="description">Die ST-Strecke weist eine Anhebung auf.</items>
                </term_definitions>
                <term_definitions code="at0104">
                    <items id="text">ST-Senkung</items>
                    <items id="description">Die ST-Strecke weist eine Absenkung auf.</items>
                </term_definitions>
                <term_definitions code="at0105">
                    <items id="text">Globaler RR-Abstand</items>
                    <items id="description">Der RR-Abstand ist der Abstand zwischen aufeinanderfolgenden R-Wellen im EKG as Ganzes (mehrere Ableitungen).</items>
                    <items id="comment">Der RR-Abstand wird als der zeitliche Abstand zwischen aufeinanderfolgenden R-Wellen gemessen.</items>
                </term_definitions>
                <term_definitions code="at0106">
                    <items id="text">ST 60ms</items>
                    <items id="description">Erhebung oder Vertiefung des ST-Segments 80 ms nach Ende des QRS-Komplexes (J-Punkt).</items>
                </term_definitions>
                <term_definitions code="at0107">
                    <items id="text">Position</items>
                    <items id="description">Die Position des Patienten zum Zeitpunkt der Messung.</items>
                </term_definitions>
                <term_definitions code="at0108">
                    <items id="text">Stehend</items>
                    <items id="description">Stehend zum Zeitpunkt der EKG Messung.</items>
                </term_definitions>
                <term_definitions code="at0109">
                    <items id="text">Sitzend</items>
                    <items id="description">Sitzend (zum Beispiel auf einem Stuhl oder Bett) zum Zeitpunkt der EKG Messung.</items>
                </term_definitions>
                <term_definitions code="at0110">
                    <items id="text">Angelehnt</items>
                    <items id="description">Angelehnte Position zum Zeitpunkt der EKG Messung.</items>
                </term_definitions>
                <term_definitions code="at0111">
                    <items id="text">Liegend</items>
                    <items id="description">Flach liegend zum Zeitpunkt der EKG Messung.</items>
                </term_definitions>
                <term_definitions code="at0112">
                    <items id="text">Liegend mit Neigung nach links</items>
                    <items id="description">Flach liegend mit einer bestimmten Neigung, meist in Richtung der linken Körperseite.</items>
                </term_definitions>
                <term_definitions code="at0113">
                    <items id="text">Weitere Details</items>
                    <items id="description">Weitere struktukturierte Details über das EKG Ergebnis.</items>
                </term_definitions>
                <term_definitions code="at0114">
                    <items id="text">Ableitungsdetails</items>
                    <items id="description">Weitere strukturierte Details über die jeweilige Ableitung.</items>
                </term_definitions>
                <term_definitions code="at0115">
                    <items id="text">Kommentar zur Ableitung</items>
                    <items id="description">Weitere Informationen über die Ergebnisse der Ableitung, die noch nicht in anderen Feldern erfasst wurden.</items>
                </term_definitions>
                <term_definitions code="at0116">
                    <items id="text">Schrittmacher Stimulation</items>
                    <items id="description">Beschreibung der Schrittmacher Aktivität.</items>
                    <items id="comment">Dieser Freitext kann im Template weiter kodiert werden um einen Vorhandensein/Nicht-Vorhandensein Wert zu ermöglichen, wenn dies für Reporting Zwecke notwendig ist.</items>
                </term_definitions>
                <term_definitions code="deg">
                    <items id="text">°</items>
                    <items id="description">degree</items>
                </term_definitions>
            </children>
            <cardinality>
                <is_ordered>false</is_ordered>
                <is_unique>false</is_unique>
                <interval>
                    <lower_included>true</lower_included>
                    <upper_included>false</upper_included>
                    <lower_unbounded>false</lower_unbounded>
                    <upper_unbounded>true</upper_unbounded>
                    <lower>1</lower>
                </interval>
            </cardinality>
        </attributes>
        <archetype_id>
            <value>openEHR-EHR-COMPOSITION.report.v1</value>
        </archetype_id>
        <template_id>
            <value>ecg_result</value>
        </template_id>
        <term_definitions code="at0000">
            <items id="text">ecg_result</items>
            <items id="description">Ein Dokument zur Übermittlung von Informationen an andere, meist als Antwort auf eine Anfrage.</items>
        </term_definitions>
        <term_definitions code="at0001">
            <items id="text">Tree</items>
            <items id="description">@ internal @</items>
        </term_definitions>
        <term_definitions code="at0002">
            <items id="text">Bericht ID</items>
            <items id="description">Identifizierungsmerkmal des Berichts.</items>
        </term_definitions>
        <term_definitions code="at0005">
            <items id="text">Status</items>
            <items id="description">Der Status des gesamten Berichts. Hinweis: Dies ist nicht der Status einer Berichtskomponente.</items>
        </term_definitions>
        <term_definitions code="at0006">
            <items id="text">Erweiterung</items>
            <items id="description">Zusätzliche Informationen zur Erfassung lokaler Inhalte oder Anpassung an andere Referenzmodelle/Formalismen.</items>
            <items id="comment">Zum Beispiel: lokaler Informationsbedarf oder zusätzliche Metadaten zur Anpassung an FHIR-Ressourcen oder CIMI-Modelle.</items>
        </term_definitions>
    </definition>
    <ontology archetype_id="openEHR-EHR-COMPOSITION.report.v1">
        <term_definitions language="en">
            <items code="at0000">
                <items id="text">ecg_result</items>
                <items id="description">Document to communicate information to others, commonly in response to a request from another party.</items>
            </items>
            <items code="at0001">
                <items id="text">Tree</items>
                <items id="description">@ internal @</items>
            </items>
            <items code="at0002">
                <items id="text">Report ID</items>
                <items id="description">Identification information about the report.</items>
            </items>
            <items code="at0005">
                <items id="text">Status</items>
                <items id="description">The status of the entire report. Note: This is not the status of any of the report components.</items>
            </items>
            <items code="at0006">
                <items id="text">Extension</items>
                <items id="description">Additional information required to capture local context or to align with other reference models/formalisms.</items>
                <items id="comment">For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.</items>
            </items>
        </term_definitions>
    </ontology>
    <component_ontologies archetype_id="openEHR-EHR-OBSERVATION.ecg_result.v1">
        <term_definitions language="en">
            <items code="at0000">
                <items id="text">ECG result</items>
                <items id="description">Measurement of the electrical activity generated by the heart.</items>
                <items id="comment">Also known as an electrocardiograph or EKG.</items>
            </items>
            <items code="at0001">
                <items id="text">Event Series</items>
                <items id="description">@ internal @</items>
            </items>
            <items code="at0002">
                <items id="text">Any event</items>
                <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
            </items>
            <items code="at0003">
                <items id="text">Tree</items>
                <items id="description">@ internal @</items>
            </items>
            <items code="at0005">
                <items id="text">Tree</items>
                <items id="description">@ internal @</items>
            </items>
            <items code="at0007">
                <items id="text">QT interval global</items>
                <items id="description">Interval measurement from the onset of the QRS complex to the end of the T wave aacross the ECG as a whole (multiple leads).</items>
            </items>
            <items code="at0008">
                <items id="text">QTc interval global</items>
                <items id="description">Correction applied to the 'QT interval global'.</items>
            </items>
            <items code="at0009">
                <items id="text">Device interpretation</items>
                <items id="description">Interpretative comment on this ECG result, originating from a device.</items>
                <items id="comment">May be multiple. Coding of the interpretation with a terminology is preferred, where possible. For example: normal sinus rhythm; ischaemic ST-T changes in anterior leads; and/or poor R progression in right precordial leads.</items>
            </items>
            <items code="at0012">
                <items id="text">PR interval global</items>
                <items id="description">PR interval measurement across the ECG as a whole (multiple leads).</items>
                <items id="comment">The PR interval is measured from onset of P wave to the onset of QRS complex.</items>
            </items>
            <items code="at0013">
                <items id="text">Ventricular heart rate</items>
                <items id="description">The frequency of ventricular electrical contractions across the ECG as a whole (multiple leads).</items>
                <items id="comment">Also known as the RR rate, QRS rate or electrical heart rate. The ventricular heart rate is measured from R wave to R wave and calculated across the ECG as a whole (multiple leads).</items>
            </items>
            <items code="at0014">
                <items id="text">QRS duration global</items>
                <items id="description">Duration of QRS complex across the ECG as a whole (multiple leads).</items>
                <items id="comment">The QRS complex is measured from its onset to &#13;
the ST segment onset (J point).</items>
            </items>
            <items code="at0020">
                <items id="text">P axis</items>
                <items id="description">Average direction of electrical activity during atrial depolarization.</items>
                <items id="comment">For example: normal axis or left deviation; or number of degrees.</items>
            </items>
            <items code="at0021">
                <items id="text">QRS axis</items>
                <items id="description">Average direction of electrical activity during ventricular depolarization.</items>
                <items id="comment">For example: normal axis or left deviation; or number of degrees.</items>
            </items>
            <items code="at0022">
                <items id="text">T axis</items>
                <items id="description">Average direction of electrical activity during ventricular repolarization.</items>
                <items id="comment">For example: normal axis or left deviation; or number of degrees.</items>
            </items>
            <items code="at0025">
                <items id="text">QTc algorithm</items>
                <items id="description">Algorithm used to correct QT interval.</items>
            </items>
            <items code="at0027">
                <items id="text">Per-lead</items>
                <items id="description">Details about measured parameters for each named lead (specified in the run-time name constraint).</items>
            </items>
            <items code="at0028">
                <items id="text">P duration</items>
                <items id="description">Duration of P wave.</items>
            </items>
            <items code="at0029">
                <items id="text">Lead I</items>
                <items id="description">Lead I is the voltage difference between the left arm electrode and right arm electrode, directed towards the left arm at zero degrees.</items>
            </items>
            <items code="at0030">
                <items id="text">Lead II</items>
                <items id="description">Lead II is the voltage difference between the left leg electrode and the right arm electrode, directed towards the left leg at +60 degrees.</items>
            </items>
            <items code="at0031">
                <items id="text">Lead III</items>
                <items id="description">Lead III is the voltage difference between the left leg electrode and the left arm electrode, directed towards the left leg at +120 degrees.</items>
            </items>
            <items code="at0032">
                <items id="text">Lead aVR</items>
                <items id="description">Lead augmented vector right (aVR) has the positive electrode on the right arm and the negative pole is a combination of the left arm electrode and the left leg electrode. It is directed towards the right arm at -150 degrees.</items>
            </items>
            <items code="at0033">
                <items id="text">Lead aVL</items>
                <items id="description">Lead augmented vector left (aVL) has the positive electrode on the left arm and the negative pole is a combination of the right arm electrode and the left leg electrode. It is directed towards the left arm electrode at -30 degrees.</items>
            </items>
            <items code="at0034">
                <items id="text">Lead aVF</items>
                <items id="description">Lead augmented vector foot (aVF) has the positive electrode on the left leg and the negative pole is a combination of the right arm electrode and the left arm electrode. It is directed towards the left leg electrode at +90 degrees.</items>
            </items>
            <items code="at0035">
                <items id="text">Lead V1</items>
                <items id="description">A precordial lead, placed to the right of the sternum in the fourth intercostal space. Also known as lead V2R.</items>
            </items>
            <items code="at0036">
                <items id="text">Lead V2</items>
                <items id="description">A precordial lead, placed to the left of the sternum in the fourth intercostal space. Also known as lead V1R.</items>
            </items>
            <items code="at0037">
                <items id="text">Lead V3</items>
                <items id="description">A precordial lead, placed directly between leads V2 and V4.</items>
            </items>
            <items code="at0038">
                <items id="text">Lead V4</items>
                <items id="description">A precordial lead, placed in the left fifth intercostal space at the midclavicular line.</items>
            </items>
            <items code="at0039">
                <items id="text">Lead V5</items>
                <items id="description">A precordial lead, placed level with lead V4 at the left anterior axillary line.</items>
            </items>
            <items code="at0040">
                <items id="text">Lead V6</items>
                <items id="description">A precordial lead, placed level with lead V5 at the left midaxillary line.</items>
            </items>
            <items code="at0041">
                <items id="text">P amplitude</items>
                <items id="description">Amplitude of the P wave.</items>
            </items>
            <items code="at0042">
                <items id="text">P area</items>
                <items id="description">Area of a monophasic P wave or the area of the initial portion of a biphasic P wave.</items>
            </items>
            <items code="at0043">
                <items id="text">P' amplitude</items>
                <items id="description">Amplitude of P' wave.</items>
            </items>
            <items code="at0044">
                <items id="text">P' duration</items>
                <items id="description">Duration of P' wave.</items>
            </items>
            <items code="at0046">
                <items id="text">P' area</items>
                <items id="description">Area of the terminal portion of a biphasic P wave.</items>
            </items>
            <items code="at0048">
                <items id="text">Q amplitude</items>
                <items id="description">Amplitude of the Q wave.</items>
            </items>
            <items code="at0049">
                <items id="text">Q duration</items>
                <items id="description">Duration of the Q wave.</items>
            </items>
            <items code="at0050">
                <items id="text">R amplitude</items>
                <items id="description">Amplitude of the R wave.</items>
            </items>
            <items code="at0051">
                <items id="text">R duration</items>
                <items id="description">Duration of the R wave.</items>
            </items>
            <items code="at0053">
                <items id="text">S amplitude</items>
                <items id="description">Amplitude of the S wave.</items>
            </items>
            <items code="at0054">
                <items id="text">S duration</items>
                <items id="description">Duration of the S wave.</items>
            </items>
            <items code="at0055">
                <items id="text">R' amplitude</items>
                <items id="description">Amplitude of the R' wave.</items>
            </items>
            <items code="at0056">
                <items id="text">R' duration</items>
                <items id="description">Duration of the R' wave.</items>
            </items>
            <items code="at0057">
                <items id="text">S' amplitude</items>
                <items id="description">Amplitude of the S' wave.</items>
            </items>
            <items code="at0058">
                <items id="text">S' duration</items>
                <items id="description">Duration of the S' wave.</items>
            </items>
            <items code="at0059">
                <items id="text">Ventricular Activation Time (VAT)</items>
                <items id="description">Interval from the onset of the QRS complex to the latest positive peak in the complex, or the latest substantial notch on the latest peak (whichever is later).</items>
            </items>
            <items code="at0060">
                <items id="text">QRS p-p</items>
                <items id="description">Amplitude of the peak-to-peak QRS complex.</items>
            </items>
            <items code="at0061">
                <items id="text">QRS duration</items>
                <items id="description">Duration of the QRS complex.</items>
                <items id="comment">Measured from its onset to the ST segment onset (J point).</items>
            </items>
            <items code="at0062">
                <items id="text">QRS area</items>
                <items id="description">Area of the QRS complex.</items>
            </items>
            <items code="at0063">
                <items id="text">ST onset</items>
                <items id="description">Elevation or depression at the onset (J point) of the ST segment.</items>
            </items>
            <items code="at0064">
                <items id="text">ST midpoint</items>
                <items id="description">Elevation or depression at the midpoint of the ST segment.</items>
            </items>
            <items code="at0065">
                <items id="text">ST 80ms</items>
                <items id="description">Elevation or depression of the ST segment 80 ms after the end of the QRS complex (J point).</items>
            </items>
            <items code="at0066">
                <items id="text">ST end</items>
                <items id="description">Elevation or depression at the end of the ST segment.</items>
            </items>
            <items code="at0067">
                <items id="text">ST duration</items>
                <items id="description">Duration of the ST segment.</items>
            </items>
            <items code="at0068">
                <items id="text">ST slope</items>
                <items id="description">Slope of the ST segment.</items>
                <items id="comment">Slope is measured in degrees for 25 mm/sec, 1mV/cm scaling.</items>
            </items>
            <items code="at0069">
                <items id="text">ST segment morphology</items>
                <items id="description">Shape of the ST segment.</items>
                <items id="comment">Value set sourced from DICOM PS3.16 2018 c - Content Mapping Resource -http://dicom.nema.org/medical/dicom/current/output/html/part16.html#sect_CID_3233</items>
            </items>
            <items code="at0070">
                <items id="text">ST depression - horizontal</items>
                <items id="description">The ST segment is depressed but not sloping. This is a more specific form of 'ST depression'.</items>
            </items>
            <items code="at0071">
                <items id="text">ST depression - upsloping</items>
                <items id="description">The ST segment is depressed and sloping upward. This is a more specific form of 'ST depression'.</items>
            </items>
            <items code="at0072">
                <items id="text">ST depression - downsloping</items>
                <items id="description">The ST segment is depressed and downsloping. This is a more specific form of 'ST depression'.</items>
            </items>
            <items code="at0073">
                <items id="text">T amplitude</items>
                <items id="description">Amplitude of the T wave.</items>
            </items>
            <items code="at0074">
                <items id="text">T duration</items>
                <items id="description">Duration of the T wave.</items>
            </items>
            <items code="at0075">
                <items id="text">T area</items>
                <items id="description">Area of the T wave.</items>
            </items>
            <items code="at0076">
                <items id="text">Recording device</items>
                <items id="description">Details about the electrocardiograph device used to record the ECG.</items>
            </items>
            <items code="at0077">
                <items id="text">Tree</items>
                <items id="description">@ internal @</items>
            </items>
            <items code="at0078">
                <items id="text">Tilt</items>
                <items id="description">The craniocaudal tilt of the surface on which the person is lying during the ECG. Lying horizontally is considered the 0 degrees position.</items>
            </items>
            <items code="at0079">
                <items id="text">Confounding factors</items>
                <items id="description">Comment on and record other incidental factors that may be contributing to ECG result.</items>
                <items id="comment">For example: Uncooperative patient; difficulty with lead placement; or moist skin.</items>
            </items>
            <items code="at0080">
                <items id="text">Level of exertion</items>
                <items id="description">Details about physical activity undertaken at the time of ECG recording.</items>
            </items>
            <items code="at0081">
                <items id="text">ECG diagnosis</items>
                <items id="description">Single word, phrase or brief description that represents the overall clinical meaning and significance of the ECG result.</items>
                <items id="comment">For example: 'posterior AMI' or 'supraventricular tachycardia'. Coding of the diagnosis with a terminology is preferred, where possible. This diagnosis should be aligned with the narrative in the 'Conclusion'.</items>
            </items>
            <items code="at0082">
                <items id="text">Viewing device</items>
                <items id="description">Details of device used to view the ECG output.</items>
            </items>
            <items code="at0083">
                <items id="text">Multimedia representation</items>
                <items id="description">Digital representation of the ECG result.</items>
                <items id="comment">The ECG may be represented as an image or another multimedia/binary type. Multiple formats are allowed but they should represent equivalent clinical content.</items>
            </items>
            <items code="at0084">
                <items id="text">Lead V7</items>
                <items id="description">Lead V7 is a posterior lead, placed in the posterior axillary line.</items>
            </items>
            <items code="at0085">
                <items id="text">Lead V8</items>
                <items id="description">Lead V8 is a posterior lead, placed in the midscapular region.</items>
            </items>
            <items code="at0086">
                <items id="text">Lead V9</items>
                <items id="description">Lead V9 is a posterior lead, placed in the paraspinal region.</items>
            </items>
            <items code="at0087">
                <items id="text">Lead V4R</items>
                <items id="description">A precordial lead, placed in the right fifth intercostal space at the midclavicular line.</items>
            </items>
            <items code="at0088">
                <items id="text">Extension</items>
                <items id="description">Additional information required to capture local content or to align with other reference models/formalisms.</items>
                <items id="comment">For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.</items>
            </items>
            <items code="at0089">
                <items id="text">Conclusion</items>
                <items id="description">Narrative synthesis about all of the key findings in the ECG result.</items>
                <items id="comment">The conclusion is intended to be a narrative and expanded version of the 'ECG diagnosis'. For example: 'Normal sinus rhythm with ischaemic ST-T changes in anterior leads and poor R progression in right precordial leads; or comparison with previous ECG results.</items>
            </items>
            <items code="at0090">
                <items id="text">Comment</items>
                <items id="description">Additional narrative about the ECG result, not captured in other fields.</items>
            </items>
            <items code="at0091">
                <items id="text">Lead V5R</items>
                <items id="description">A precordial lead, placed level with lead V4R at the right anterior axillary line.</items>
            </items>
            <items code="at0092">
                <items id="text">Lead V6R</items>
                <items id="description">A precordial lead, placed level with lead V5R at the right midaxillary line.</items>
            </items>
            <items code="at0093">
                <items id="text">Lead V3R</items>
                <items id="description">A precordial lead, placed directly on the right side of the chest between leads V1 and V4R.</items>
            </items>
            <items code="at0094">
                <items id="text">Atrial heart rate</items>
                <items id="description">The frequency of atrial electrical contractions across the ECG as a whole (multiple leads).</items>
                <items id="comment">Also known as the PP rate. The atrial heart rate is measured from P wave to P wave.</items>
            </items>
            <items code="at0095">
                <items id="text">Device interpretation comment</items>
                <items id="description">Comment about the interpretation by the device.</items>
                <items id="comment">For example: identification of the algorithm used; or description of technical limitations.</items>
            </items>
            <items code="at0096">
                <items id="text">Clinical information provided</items>
                <items id="description">Narrative description of clinical information available at the time of interpretation of results.</items>
                <items id="comment">This data element may include a link to the original clinical information provided in the ECG request.</items>
            </items>
            <items code="at0097">
                <items id="text">ECG lead placement</items>
                <items id="description">Lead placement for the ECG recording.</items>
                <items id="comment">For example: 'Standard 12-lead extended to the right by V5R, V4R, V3R'. Coding of the lead placement with a terminology, such as DICOM CID 3263/ISO 11073 MDC codes, is desirable where possible.</items>
            </items>
            <items code="at0098">
                <items id="text">Description</items>
                <items id="description">Narrative description about the findings for the specified lead.</items>
                <items id="comment">For example: ST segment findings.</items>
            </items>
            <items code="at0100">
                <items id="text">ECG type</items>
                <items id="description">Type of ECG performed.</items>
                <items id="comment">Coding of the ECG type with a terminology is preferred, where possible. For example: 'Standard ECG'; or '15-lead ECG'.</items>
            </items>
            <items code="at0101">
                <items id="text">Finding</items>
                <items id="description">Single word, phrase or brief description that represents a significant finding in the ECG result.</items>
                <items id="comment">For example: 'Normal ECG', 'Equivocal ECG' or 'Abnormal ECG'; 'Spikes from pacemaker signal'; 'ST depression'; or 'Atrial fibrillation'. Coding of the finding with a terminology is preferred, where possible.</items>
            </items>
            <items code="at0102">
                <items id="text">Technical quality</items>
                <items id="description">Single word, phrase or brief description that represents a significant technical quality issue that impacts the ECG result.</items>
                <items id="comment">For example: codes from IHE ECG codetable, such as 'Incomplete ECG'; 'Limb electrodes interchanged'; or 'Muscle tremor'.</items>
            </items>
            <items code="at0103">
                <items id="text">ST elevation</items>
                <items id="description">The ST segment is elevated.</items>
            </items>
            <items code="at0104">
                <items id="text">ST depression</items>
                <items id="description">The ST segment is depressed.</items>
            </items>
            <items code="at0105">
                <items id="text">RR interval global</items>
                <items id="description">The interval between successive R waves across the ECG as a whole (multiple leads).</items>
                <items id="comment">The RR interval is measured as the time interval between successive R waves.</items>
            </items>
            <items code="at0106">
                <items id="text">ST 60ms</items>
                <items id="description">Elevation or depression of the ST segment 60 ms after the end of the QRS complex (J point).</items>
            </items>
            <items code="at0107">
                <items id="text">Position</items>
                <items id="description">The position of the subject at the time of measurement.</items>
            </items>
            <items code="at0108">
                <items id="text">Standing</items>
                <items id="description">Standing at the time of ECG testing.</items>
            </items>
            <items code="at0109">
                <items id="text">Sitting</items>
                <items id="description">Sitting (for example on bed or chair) at the time of ECG testing.</items>
            </items>
            <items code="at0110">
                <items id="text">Reclining</items>
                <items id="description">Reclining at the time of ECG testing.</items>
            </items>
            <items code="at0111">
                <items id="text">Lying</items>
                <items id="description">Lying flat at the time of ECG testing.</items>
            </items>
            <items code="at0112">
                <items id="text">Lying with tilt to left</items>
                <items id="description">Lying flat with some lateral tilt, usually angled towards the left side at the time of ECG testing.</items>
            </items>
            <items code="at0113">
                <items id="text">Additional details</items>
                <items id="description">Additional structured details about the ECG result.</items>
            </items>
            <items code="at0114">
                <items id="text">Lead details</items>
                <items id="description">Additional structured details about the specified lead.</items>
            </items>
            <items code="at0115">
                <items id="text">Lead comment</items>
                <items id="description">Additional narrative about the lead results, not captured in other fields.</items>
            </items>
            <items code="at0116">
                <items id="text">Pacemaker stimulation</items>
                <items id="description">Narrative description about pacemaker activity.</items>
                <items id="comment">This free text field could be further coded in a template to enable a Present/Absent value set if required for reporting purposes.</items>
            </items>
            <items code="deg">
                <items id="text">°</items>
                <items id="description">degree</items>
            </items>
        </term_definitions>
    </component_ontologies>
</template>
